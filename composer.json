{
	"name" : "slothsoft/schema.slothsoft.net",
	"type" : "project",
	"description" : "schema.slothsoft.net",
	"license" : "WTFPL",
	"keywords" : [
		"slothsoft",
		"farah-module",
		"farah-server"
	],
	"autoload" : {
		"files" : [
			"config.php",
			"scripts/bootstrap.php"
		]
	},
	"config": {
       "platform": {
           "php": "7.4"
       }
    },
	"require" : {
		"php" : "7.4 - 8.0",
		"slothsoft/core" : "^1.5",
		"slothsoft/farah" : "^1.6",
		"slothsoft/schema" : "^1.1",
		"slothsoft/amber" : "^1.1",
		"slothsoft/cron" : "^1.0",
		"slothsoft/savegame" : "^1.0"
	},
	"require-dev" : {
		"phpunit/phpunit" : "8.5.*"
	},
	"autoload-dev" : {
		"psr-4" : {
			"Slothsoft\\Server\\Schema\\" : "tests"
		}
	},
	"scripts" : {
		"post-autoload-dump" : "Slothsoft\\Core\\ServerEnvironment::cleanCacheDirectory"
	},
	"homepage" : "http://schema.slothsoft.net",
	"authors" : [{
			"name" : "Daniel Schulz",
			"email" : "info.slothsoft@gmail.com"
		}
	]
